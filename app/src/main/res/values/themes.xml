<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ServiceCars" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryDark">@color/secondaryColor</item>
        <item name="colorAccent">@color/primaryLightColor</item>
    </style>

    <style name="Theme.ServiceCars.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.ServiceCars.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.ServiceCars.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>